{
  "type": "module",
  "name": "todomvc-screenreader",
  "description": "TodoMVC for screen readers build with @depository",
  "version": "0.58.0",
  "author": "Sune Simonsen",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "relivestyle --single --port 5001 public",
    "build": "rm -rf dist && rollup -c node:@depository/rollup-config",
    "deploy": "yarn build && surge dist depository-todomvc-screenreader.surge.sh"
  },
  "dependencies": {
    "@depository/function-middleware": "^0.58.0",
    "@depository/store": "^0.58.0",
    "@depository/todomvc-api": "^0.58.0",
    "@depository/todomvc-model": "^0.58.0",
    "@depository/view": "^0.58.0",
    "@nano-router/depository-view": "^2.7.0",
    "@nano-router/history": "^2.2.0",
    "@rollup/plugin-html": "^0.2.3",
    "htm": "^3.0.4",
    "preact": "^10.5.7"
  },
  "devDependencies": {
    "@depository/rollup-config": "^0.58.0",
    "relivestyle": "1.0.0-6",
    "rollup": "^2.39.0",
    "surge": "^0.23.0"
  }
}
